{
  "manifest_version": 3,
  "name": "ZipLink",
  "version": "1.1.0",
  "description": "ZipLink",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "ZipLink",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:5173/*",
    "https://web-ashen-psi.vercel.app/*"
  ]

}